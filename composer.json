{
    "name": "yiisoft/json",
    "type": "library",
    "description": "json",
    "keywords": [
        "json"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/json/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/json"
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "infection/infection": "^0.16.3 || ^0.17.0",
        "phan/phan": "^3.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Json\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Json\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
